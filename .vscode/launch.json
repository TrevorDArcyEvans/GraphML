{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "IdentityServerAspNetIdentity",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run"
      ],
      "cwd": "${workspaceFolder}/IdentityServerAspNetIdentity",
      "env": {},
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "API",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run"
      ],
      "cwd": "${workspaceFolder}/GraphML.API",
      "env": {},
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "GTK Linux",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run"
      ],
      "cwd": "${workspaceFolder}/GraphML.UI.Uno/GraphML.UI.Uno.Skia.Gtk",
      "env": {},
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": ".NET Core Launch (Uno Platform App)",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": [
        "run"
      ],
      "cwd": "${workspaceFolder}/GraphML.UI.Uno/GraphML.UI.Uno.Wasm",
      "env": {}
    },
    {
      "name": ".NET Core Debug Uno Platform WebAssembly in Chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "timeout": 30000,
      // If you have changed the default port / launch URL make sure to update the expectation below
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}/GraphML.UI.Uno/GraphML.UI.Uno.Wasm",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "runtimeExecutable": "/usr/bin/google-chrome-stable"
    }
  ]
}